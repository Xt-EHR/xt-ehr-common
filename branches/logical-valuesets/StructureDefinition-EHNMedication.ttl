       fhir:path [ fhir:v "EHNMedication.unitOfPresentation" ] ;
       fhir:short [ fhir:v "Unit of presentation" ] ;
       fhir:definition [ fhir:v "Unit of presentation is used as a unit when describing the strength, but according to ISO IDMP it is also a separate attribute of the pharmaceutical product and manufactured item." ] ;
